{
  "name": "recipe_prep-frontend",
  "version": "1.0.0",
  "engines": {
    "node": "16.x"
  },
  "description": "Project for CSE 115A",
  "author": "Tyler Hayden, Adam Barsness, Emmanuel Pena, Shane Smith, Joshua Harding ",
  "license": "UNLICENSED",
  "repository": "https://git.ucsc.edu/jorhardi/cse-115a-meal-prep",
  "proxy": "http://localhost:3010",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "@mui/icons-material": "*",
    "@mui/material": "^5.3.1",
    "@react-oauth/google": "^0.7.0",
    "emoji-picker-react": "*",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "notistack": "^2.0.5",
    "prop-types": "*",
    "react": "*",
    "react-big-calendar": "^0.38.9",
    "react-dom": "^17.0.2",
    "react-google-button": "^0.7.2",
    "react-google-login": "^5.2.2",
    "react-router": "5.2.1",
    "react-router-dom": "^6.2.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "bestzip": "*",
    "eslint-config-google": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "msw": "*",
    "puppeteer": "*",
    "puppeteer-core": "*",
    "react-scripts": "^5.0.1",
    "typescript": "4.7.4"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test --coverage",
    "lint": "eslint ./src/**/*.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "require-jsdoc": "off",
      "no-unused-vars": "off",
      "prefer-rest-params": "off",
      "indent": "off",
      "max-len": "off",
      "no-undef": "off",
      "camelcase": "off",
      "linebreak-style": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
